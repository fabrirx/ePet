<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0EAD3"
    tools:context=".telaInformacoes">

    <ImageView
        android:id="@+id/imgPerfil"
        android:layout_width="95dp"
        android:layout_height="101dp"
        app:srcCompat="@drawable/usario"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/txtPerfil"
        android:layout_width="67dp"
        android:layout_height="26dp"
        android:fontFamily="@font/poppins_bold"
        android:text="PERFIL"
        android:textColor="#000000"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="169dp" />

    <ImageView
        android:id="@+id/imgRetangulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/retangulo"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="151dp" />

    <EditText
        android:id="@+id/editTxtNome"
        android:layout_width="282dp"
        android:layout_height="28dp"
        android:background="#ADC17C"
        android:backgroundTint="#ADC17C"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="228dp" />

    <EditText
        android:id="@+id/editTxtSobrenome"
        android:layout_width="282dp"
        android:layout_height="28dp"
        android:background="#ADC17C"
        android:backgroundTint="#ADC17C"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="299dp" />

    <EditText
        android:id="@+id/editTxtCPF"
        android:layout_width="282dp"
        android:layout_height="28dp"
        android:background="#ADC17C"
        android:backgroundTint="#ADC17C"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="360dp" />

    <TextView
        android:id="@+id/txtNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="202dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Nome"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="64dp" />

    <TextView
        android:id="@+id/txtSobrenome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="270dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Sobrenome"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="64dp" />

    <TextView
        android:id="@+id/txtCpf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="338dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="CPF"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="64dp" />

    <TextView
        android:id="@+id/txtTelefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="Telefone"
        android:textColor="#000000"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="397dp" />

    <EditText
        android:id="@+id/editTxtPhone"
        android:layout_width="282dp"
        android:layout_height="28dp"
        android:layout_marginTop="422dp"
        android:background="#ADC17C"
        android:backgroundTint="#ADC17C"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="64dp" />

    <TextView
        android:id="@+id/txtCep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_semibold"
        android:text="CEP"
        android:textColor="#000000"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="457dp" />

    <EditText
        android:id="@+id/editTxtCep"
        android:layout_width="282dp"
        android:layout_height="28dp"
        android:layout_marginTop="481dp"
        android:background="#ADC17C"
        android:backgroundTint="#ADC17C"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="64dp" />
</androidx.constraintlayout.widget.ConstraintLayout>